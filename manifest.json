{
  "name": "Myanmar Exchange Rate Checker",
  "description": "Check Exchange Rate for Various Myanmar Bank",
  "version": "1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
